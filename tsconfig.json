{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "lib": ["DOM", "ES2021"]
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts"]
}


